  <%= simple_form_for(@photographer) do |f| %>
    <%= f.input :avatar %>
    <%= f.input :avatar_cache, as: :hidden %>
    <%= f.input :description %>
    <%= f.input :city %>
    <%= f.input :hourly_rate %>
    <%= f.fields_for :photos do |p| %>
      <%= f.file_field :photo, multiple: true, name: "photos[photo][]" %>
    <% end %>
    <%= f.submit %>
  <% end %>
