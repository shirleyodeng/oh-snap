  <%= simple_form_for(@photographer) do |f| %>
    <%= f.input :description %>
    <%= f.input :city %>
    <%= f.input :hourly_rate %>
    <%#= f.input :photo %>
    <%#= f.input :photo_cache, as: :hidden %>
    <%= f.fields_for :photographer_photos do |p| %>
      <%= f.file_field :avatar, multiple: true, name: "photographer_photos[avatar][]" %>
    <% end %>
    <%= f.submit %>
  <% end %>
